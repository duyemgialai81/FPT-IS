{
  "name": "FPT-IS",
  "version": "1.0.0",
  "author": "Don Jade Roy",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.donjaderoy.fptis",
    "productName": "FPT-IS",
    "publish": [
      {
        "provider": "github",
        "owner": "duyemgialai81",
        "repo": "FPT-IS"
      }
    ],
    "win": {
      "target": "nsis",
      "icon": "icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "icon.ico",
      "uninstallerIcon": "icon.ico",
      "uninstallDisplayName": "FPT-IS",
      "createDesktopShortcut": true
    }
  },
  "dependencies": {
    "axios": "^1.13.2",
    "electron-updater": "^6.7.3"
  },
  "devDependencies": {
    "electron": "^39.2.7",
    "electron-builder": "^26.4.0"
  }
}